  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "plugins": ["flowtype"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:flowtype/recommended"
  ],
  "globals": {
    "Promise": true
  },
  "rules": {
    "no-console": 0
  }
}
